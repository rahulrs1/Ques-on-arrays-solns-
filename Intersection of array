// Find intersection of elements in array
#include<iostream>
using namespace std;

int main(){
    int n1,n2; // Size of arrays
    cin>>n1>>n2;
    int A1[n1],A2[n2];
    // input of elements of array 1
    for(int i=0; i<n1; i++){
        cin>>A1[i];
    }
     cout<<"elements of array 1 "<<endl;
    for(int i=0; i<n1; i++){
        cout<<A1[i]<<" ";
    }
    
    cout<<endl;
     // input of elements of array 2 
    for(int i=0; i<n1; i++){
        cin>>A2[i];
    }
    cout<<"elements of array 2 "<<endl;
    for(int i=0; i<n1; i++){
        cout<<A2[i]<<" ";
    }
    cout<<endl;
    cout<<"Common elements will be "<<endl;
    for(int i=0; i<n1; i++){
        for(int j=0; j<n2; j++){
            if(A1[i]==A2[j]){
                cout<<A1[i]<<" ";
            }
        }
    }
    return 0;
}
